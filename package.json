{
  "name": "@mike-works/pwa-fundamentals",
  "version": "0.0.0-development",
  "description": "A grocery shopping progressive web app",
  "main": "server/cli/cli.js",
  "repository": "https://github.com/mike-north/pwa-workshop.git",
  "contributors": [
    "Mike North <michael.l.north@gmail.com> (https://mike.works)",
    "Steve Kinney <hello@stevekinney.net> (https://stevekinney.net)"
  ],
  "engines": {
    "node": ">= 6.11.5"
  },
  "private": true,
  "license": "SEE LICENSE IN LICENSE and LICENSE-exercises",
  "scripts": {
    "watch:api": "./node_modules/.bin/nodemon -e js --watch ./server/api-server server/api-server/index.js",
    "watch:ui": "./node_modules/.bin/nodemon -e js --watch ./server/asset-server server/asset-server/index.js",
    "watch": "concurrently --names 'API,UI' -c 'bgBlue,bgMagenta' 'npm run watch:api' 'npm run watch:ui'",
    "start:api": "node server/api-server/index.js",
    "start:ui": "node server/asset-server/index.js",
    "start": "concurrently --names 'API,UI' -c 'bgBlue,bgMagenta' 'npm run start:api' 'npm run start:ui'",
    "build:dev": " ./node_modules/.bin/webpack --config webpack.config.js --env dev",
    "build:prod": " ./node_modules/.bin/webpack --config webpack.config.js --env prod",
    "clean": "rm -rf dist/* db/development.sqlite && npm run prepcerts",
    "test": "jest",
    "prepvapid": "web-push generate-vapid-keys --json > ./private/vapid.json",
    "prepcerts": "npm run prepvapid && node ./server/gen-certs.js && rm -rf dist/*.pem && mkdir -p dist && cp private/*.pem dist/",
    "heroku-postbuild": "npm run build:prod",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "@types/sequelize": "^4.27.24",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.5.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.0.0",
    "jest-cli": "^23.0.0",
    "muicss": "^0.9.39",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.3",
    "qrcode-reader": "^1.0.4",
    "react": "^16.4.2",
    "react-dom": "^16.5.0",
    "react-hot-loader": "^4.3.4",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.4.2",
    "sass-loader": "^7.1.0",
    "style-ext-html-webpack-plugin": "^3.4.7",
    "style-loader": "^0.23.0",
    "travis-deploy-once": "^5.0.7",
    "webpack": "^3.0.0",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-dev-middleware": "^2.0.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-manifest-plugin": "^2.0.0",
    "worker-loader": "^2.0.0"
  },
  "optionalDependencies": {
    "semantic-release": "^15.9.14",
    "@mike-works/workshop-semantic-release-config": "^1.0.0"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "concurrently": "^4.0.1",
    "cors": "^2.8.4",
    "debug": "^4.0.1",
    "devcert-with-localhost": "^0.3.2",
    "express": "^4.16.3",
    "idb": "^2.1.3",
    "moment": "^2.22.2",
    "sequelize": "^4.38.0",
    "sqlite3": "^4.0.0",
    "web-push": "^3.3.2"
  }
}
